{
  "name": "2025-starter",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "---------- Dev --------------------------------------------------------": "",
    "dev": "next dev --turbopack",
    "---------- Prod -------------------------------------------------------": "",
    "build": "next build",
    "start": "next start",
    "---------- Lint -------------------------------------------------------": "",
    "lint": "biome check",
    "lint:fix": "biome check --write",
    "ts": "tsc --noEmit",
    "ts:watch": "npm run ts -- --watch",
    "---------- Test --------------------------------------------------------": "",
    "test": "bun test",
    "test:watch": "bun test --watch",
    "test:coverage": "bun test --coverage",
    "---------- Helper -----------------------------------------------------": "",
    "deps": "bunx taze --interactive --recursive --install --write --timediff false",
    "deps:major": "bun run deps -- major",
    "checkall": "bun run lint:fix && bun run ts && bun run test",
    "-----------------------------------------------------------------------": ""
  },
  "dependencies": {
    "@uidotdev/usehooks": "2.4.1",
    "http-status-codes": "2.3.0",
    "next": "15.3.0",
    "react": "19.1.0",
    "react-dom": "19.1.0"
  },
  "devDependencies": {
    "@biomejs/biome": "1.9.4",
    "@tailwindcss/postcss": "4.1.4",
    "@types/node": "20.17.30",
    "@types/react": "19.1.2",
    "@types/react-dom": "19.1.2",
    "tailwindcss": "4.1.4",
    "typescript": "5.8.3",
    "@types/bun": "latest"
  }
}
